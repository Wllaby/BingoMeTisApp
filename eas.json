{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "ios": {
        "bundleIdentifier": "com.bingometis.app"
      },
      "android": {
        "buildType": "app-bundle",
        "package": "com.bingometis.app"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6739162085"
      },
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal"
      }
    }
  }
}